graph = {
'1': {'2': 1, '5': 3},
'2': {'3': 8, '4': 7, '5': 1},
'3': {'4': 1, '5': -5},
'4': {'3': 2},
'5': {'4': 4},
}
print("Enter start vertex pleaseðŸ˜¢: ")
start_vertex = input()
print("Distance from vertex", start_vertex)
print(ford_bellman(graph, start_vertex))
